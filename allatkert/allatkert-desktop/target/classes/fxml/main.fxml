<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.alkfej.controller.MainController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Kilépés">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onExit" text="Kilép" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" prefHeight="34.0" prefWidth="535.0" text="Üdvözöllek az szegedi Vadaspark rendszerében " textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="23.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin></Label>
            <Button mnemonicParsing="false" onAction="#onOrokbefogadok" prefHeight="44.0" prefWidth="250.0" text="Örökbefogadók">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
               <font>
                  <Font size="13.0" />
               </font></Button>
            <Button mnemonicParsing="false" onAction="#onAllatok" prefHeight="44.0" prefWidth="250.0" text="Örökbefogadható Állatok">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#onKonyveles" prefHeight="44.0" prefWidth="250.0" text="Örökbefogadási könyvelés" />
         </children>
      </VBox>
   </center>
</BorderPane>
