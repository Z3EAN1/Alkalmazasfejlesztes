<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.alkfej.controller.MainOrokbefogadoController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Fájl">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#onBack" text="Vissza a főmenübe" />
              <MenuItem mnemonicParsing="false" onAction="#onExit" text="Kilép" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" prefHeight="34.0" prefWidth="825.0" text="Örökbefogadók listája">
               <font>
                  <Font name="System Bold" size="23.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <TableView fx:id="emberTabla" prefHeight="343.0" prefWidth="600.0">
              <columns>
                <TableColumn fx:id="nevColumn" prefWidth="253.20001524686813" text="Neve" />
                <TableColumn fx:id="emailColumn" prefWidth="328.3999481201172" text="E-mail címe" />
                  <TableColumn fx:id="muveletekColumn" prefWidth="216.79998779296875" text="Műveletek" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </center>
</BorderPane>
